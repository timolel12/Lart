{
  "name": "lart",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run bump:patch && npm run setversion && ng serve",
    "build": "npm run bump:patch && npm run setversion && ng build",
    "publish": "npm run bump:minor && npm run setversion && ng build --configuration production && npm run postbuild && npm run copy",
    "format": "npx prettier --write src/**/*.{js,ts,html,scss}",
    "watch": "ng build --watch --configuration development",
    "copy": "copyfiles CNAME docs/",
    "postbuild": "node -e \"require('ncp').ncp('docs/index.html', 'docs/404.html', function (err) { if (err) { return console.error(err); } console.log('Datei erfolgreich kopiert!'); })\"",
    "setversion": "node scripts/set-version.js",
    "bump:patch": "node scripts/bump-version.js patch",
    "bump:minor": "node scripts/bump-version.js minor",
    "bump:major": "node scripts/bump-version.js major",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.14",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "emailjs-com": "^3.2.0",
    "ngx-owl-carousel-o": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "webpack-dev-middleware": "^7.2.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@types/jasmine": "~5.1.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ncp": "^2.0.0",
    "typescript": "~5.5.4"
  }
}
